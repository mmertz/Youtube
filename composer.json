{
  "name": "mmertz/youtube-api",
  "description": "Laravel PHP Facade/Wrapper for the Youtube Data API v3",
  "keywords": ["youtube", "api", "video", "laravel"],
  "license": "MIT",
  "authors": [
    {
      "name": "Mitch Mertz"
    }
  ],
  "require": {
    "php": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1"
  },
  "autoload": {
    "psr-4": {
      "mmertz\\Youtube\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "mmertz\\Youtube\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "mmertz\\Youtube\\YoutubeServiceProvider"
      ],
      "aliases": {
        "Youtube": "mmertz\\Youtube\\Facades\\Youtube"
      }
    }
  }
}
